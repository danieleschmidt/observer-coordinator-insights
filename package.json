{
  "name": "observer-coordinator-insights",
  "version": "0.1.0",
  "description": "Multi-agent orchestration for organizational analytics from Insights Discovery data",
  "main": "src/main.py",
  "scripts": {
    "test": "python -m pytest tests/ -v",
    "test:unit": "python -m pytest tests/unit/ -v",
    "test:integration": "python -m pytest tests/integration/ -v",
    "test:e2e": "python -m pytest tests/e2e/ -v",
    "lint": "ruff check src/ tests/",
    "format": "ruff format src/ tests/",
    "typecheck": "mypy src/",
    "coverage": "python -m pytest --cov=src --cov-report=html --cov-report=term",
    "dev": "python src/main.py",
    "build": "python -m build",
    "clean": "rm -rf build/ dist/ .pytest_cache/ htmlcov/ .coverage"
  },
  "dependencies": {
    "PyYAML": ">=6.0",
    "pandas": ">=2.0.0",
    "numpy": ">=1.24.0",
    "scikit-learn": ">=1.3.0",
    "matplotlib": ">=3.7.0",
    "seaborn": ">=0.12.0"
  },
  "devDependencies": {
    "pytest": ">=7.4.0",
    "pytest-cov": ">=4.1.0",
    "ruff": ">=0.0.291",
    "mypy": ">=1.5.0",
    "build": ">=0.10.0"
  },
  "keywords": [
    "insights-discovery",
    "clustering", 
    "team-composition",
    "organizational-analytics",
    "multi-agent",
    "orchestration"
  ],
  "author": "Terragon Labs",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/terragon-labs/observer-coordinator-insights.git"
  },
  "bugs": {
    "url": "https://github.com/terragon-labs/observer-coordinator-insights/issues"
  },
  "homepage": "https://github.com/terragon-labs/observer-coordinator-insights#readme"
}